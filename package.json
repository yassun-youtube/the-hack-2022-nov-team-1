{
  "name": "steadfast-news",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "concurrently \"next dev --turbo\" \"tailwindcss -i src/styles/globals.css -o src/styles/dist.css -w\"",
    "build": "tailwindcss -m -i src/styles/globals.css -o src/styles/dist.css && next build",
    "start": "next start",
    "lint": "eslint --cache src",
    "format": "next lint --fix --dir .",
    "prepare": "husky install",
    "test": "jest",
    "watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "storybook": "npx sb dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "lint-staged": "lint-staged - config .lintstagedrc",
    "commit": "npx czg emoji",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "@steadfast/eslint-config": "workspace:*",
    "@babel/core": "^7.22.15",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/types": "^17.4.4",
    "@heroicons/react": "^2.0.18",
    "@jest/globals": "^29.6.4",
    "@jest/types": "^29.6.3",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addons": "^7.4.0",
    "@storybook/client-api": "^7.4.0",
    "@storybook/nextjs": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-vite": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@types/babel__core": "^7.20.1",
    "@types/jest": "^29.5.4",
    "@types/lint-staged": "^13.2.0",
    "@types/node": "20.5.9",
    "@types/prettier": "^2.7.3",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/sanitize-html": "^2.9.0",
    "@types/sharp": "^0.31.1",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "commitlint-config-gitmoji": "^2.3.1",
    "concurrently": "^8.2.1",
    "cypress": "^13.1.0",
    "czg": "^1.7.1",
    "globals": "^13.21.0",
    "husky": "^8.0.3",
    "javascript-time-ago": "^2.5.9",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "server-only": "^0.0.1",
    "sharp": "^0.32.5",
    "storybook": "^7.4.0",
    "storybook-addon-next-router": "^4.0.2",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.4.9",
    "webpack": "^5.88.2",
    "zustand": "^4.4.1"
  },
  "browserslist": [
    "> 1% in JP",
    "last 2 versions"
  ],
  "resolutions": {
    "snapdragon": "0.12.1",
    "magic-string": "^0.27.0",
    "micromatch": "^4.0.5"
  },
  "validate-branch-name": {
    "pattern": "^(main){1}$|^(feature|fix)/.+$",
    "errorMsg": "ブランチ名を確認して下さい"
  },
  "engines": {
    "node": "18.17.1"
  },
  "volta": {
    "node": "18.17.1"
  },
  "pnpm": {
    "overrides": {
      "snapdragon": "^0.12.1",
      "magic-string": "^0.27.0",
      "micromatch": "^4.0.5"
    },
    "peerDependencyRules": {
      "allowAny": [
        "vite",
        "cypress"
      ]
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,scss,md,html,json}": "prettier --cache --write"
  }
}
