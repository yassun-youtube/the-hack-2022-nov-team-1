{
  "name": "steadfast-news",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss -i src/styles/globals.css -o src/styles/dist.css -w\"",
    "build": "tailwindcss -m -i src/styles/globals.css -o src/styles/dist.css && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "next lint --fix --dir .",
    "prepare": "husky install",
    "test": "jest",
    "watch": "jest --watch",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "storybook": "npx sb dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "lint-staged": "lint-staged - config .lintstagedrc",
    "commit": "npx czg emoji"
  },
  "dependencies": {
    "next": "13.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/types": "^17.4.4",
    "@heroicons/react": "^2.0.16",
    "@jest/types": "^29.4.3",
    "@next/eslint-plugin-next": "^13.1.6",
    "@next/font": "^13.1.6",
    "@storybook/addon-essentials": "^7.0.0-beta.50",
    "@storybook/addon-interactions": "^7.0.0-beta.50",
    "@storybook/addon-links": "^7.0.0-beta.50",
    "@storybook/nextjs": "^7.0.0-beta.50",
    "@storybook/react": "^7.0.0-beta.50",
    "@storybook/react-vite": "^7.0.0-beta.50",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@testing-library/cypress": "^8.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/eslint": "8.21.1",
    "@types/node": "18.14.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/sanitize-html": "^2.8.0",
    "@types/sharp": "^0.31.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "commitlint-config-gitmoji": "^2.3.1",
    "concurrently": "^7.6.0",
    "cypress": "^12.6.0",
    "czg": "^1.5.0",
    "eslint": "8.34.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "husky": "^8.0.3",
    "javascript-time-ago": "^2.5.9",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sanitize-html": "^2.10.0",
    "server-only": "^0.0.1",
    "sharp": "^0.31.3",
    "storybook": "^7.0.0-beta.50",
    "storybook-addon-next-router": "^4.0.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "4.9.5",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.1.2",
    "zustand": "^4.3.3"
  },
  "browserslist": [
    "> 1% in JP",
    "last 2 versions"
  ],
  "resolutions": {
    "snapdragon": "0.12.1",
    "magic-string": "^0.27.0",
    "micromatch": "^4.0.5"
  },
  "validate-branch-name": {
    "pattern": "^(main){1}$|^(feature|fix)/.+$",
    "errorMsg": "ブランチ名を確認して下さい"
  },
  "engines": {
    "yarn": "please-use-npm"
  },
  "volta": {
    "node": "18.14.1"
  }
}
